{
  "source": "./src",
  "destination": "./esdoc",
  "package.json": "./package.json",
  "plugins": [
    {
      "name": "esdoc-typescript-plugin",
      "option": {
        "enable":
        true
      }
    },
    {
      "name": "esdoc-publish-html-plugin"
    },
    {
      "name": "esdoc-lint-plugin",
      "option": {
        "enable": true
      }
    },
    {
      "name": "esdoc-brand-plugin",
      "option": {
        "logo": "./resources/icon.png",
        "title": "D3.js Ionic",
        "description": "D3.js in a Ionic mobile application: examples",
        "repository": "https://github.com/proustibat/d3js-ionic",
        "site": "https://play.google.com/store/apps/details?id=prstbt.ionic.d3",
        "author": "https://twitter.com/proustibat",
        "image": "https://lh3.googleusercontent.com/8kGUUoRIP498DlmrqpW80jKmwhXzOpJWqBuo0vFlnvXlcVJekuE3c7Kch3avFDsa7SM=w300-rw"
      }
    },
    {
      "name": "esdoc-unexported-identifier-plugin",
      "option": {
        "enable": false
      }
    },
    {
      "name": "esdoc-undocumented-identifier-plugin",
      "option": {
        "enable": true
      }
    }
  ]
}
